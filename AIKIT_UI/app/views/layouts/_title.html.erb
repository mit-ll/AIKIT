<table>
  <thead></thead>
  <tbody>
    <tr>
      <td width="30"><%= image_tag("MITLL_logo.png", height: 50, width: 40) %></td>
      <td>
        <span class="headerTitleLarge">ICE<sup>3</sup></span>
        <% if session[:level].nil? || session[:level] < 1 -%>
          <span class="headerTitleNext">&nbsp;LLM</span>
        <% else -%>
          <% if session[:level] < 7 -%>
            <span class="headerTitleNext">&nbsp;LLM RAG</span>
          <% else -%>
            <span class="headerTitleNext">&nbsp;AIKIT</span>
          <% end -%>
        <% end -%>
        <span class="headerTitleSub">&nbsp; version 0.1, Level <%= session[:level] %>&nbsp</span>
        <%= link_to "+", :id => 1, :controller => :llm_questions, :action => :level_plus %>
        <%= link_to "-", :id => 1, :controller => :llm_questions, :action => :level_minus  %>
      </td>
    </tr>
  </tbody>
</table>
